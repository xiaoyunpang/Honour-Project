<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>nsercServer</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
        <servlet-name>nserc</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>nserc</servlet-name>
        <url-pattern>/welcome.jsp</url-pattern>
        <url-pattern>/welcome.html</url-pattern>
        <url-pattern>/welcomeP.html</url-pattern>
        <url-pattern>/welcomeN.html</url-pattern>
        <url-pattern>/welcomeS.html</url-pattern>
        <url-pattern>/about.jsp</url-pattern>
        <url-pattern>/about.html</url-pattern>
        <url-pattern>/year.jsp</url-pattern>
        <url-pattern>/year.html</url-pattern>
        <url-pattern>/yearP.html</url-pattern>
        <url-pattern>/yearN.html</url-pattern>
        <url-pattern>/yearS.html</url-pattern>
        <url-pattern>/institution.jsp</url-pattern>
        <url-pattern>/institution.html</url-pattern>
        <url-pattern>/institutionP.html</url-pattern>
        <url-pattern>/institutionN.html</url-pattern>
        <url-pattern>/summary.jsp</url-pattern>
        <url-pattern>/summary.html</url-pattern>
        <url-pattern>/summaryP.html</url-pattern>
        <url-pattern>/summaryN.html</url-pattern>
        <url-pattern>/summaryO.html</url-pattern>
    </servlet-mapping>
</web-app>